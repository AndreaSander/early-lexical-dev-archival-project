mutate (p.value = format_value(p.value, "p")) %>%
write_csv(file = "supplementary_tables/age_gam.csv")
age_mer <-  age_mer %>% as.data.frame() %>%
write_csv(file = "supplementary_tables/age_mer.csv")
##Experience tables
