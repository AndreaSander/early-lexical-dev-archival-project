mutate (p.value = format_value(p.value, "p")) %>%
write_csv(file = "supplementary_tables/exp_gam.csv")
exp_mer <-  exp_mer %>% as.data.frame() %>%
write_csv(file = "supplementary_tables/exp_mer.csv")
## Additive tables
